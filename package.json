{
  "name": "sign-company-dashboard",
  "version": "1.0.0",
  "description": "Modern internal dashboard for Sign Company Owners",
  "main": "backend/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cross-env PORT=9000 nodemon backend/index.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:gh-pages": "./scripts/build-gh-pages.sh",
    "start": "node backend/index.js",
    "install-all": "npm install && cd frontend && npm install --production=false",
    "postinstall": "cd frontend && npm install --production=false",
    "deploy:github": "./scripts/deploy-to-github.sh",
    "seed:events": "node backend/seedEvents.js",
    "seed:admin": "node backend/seedAdmin.js",
    "seed:vendor": "node backend/seedVendor.js",
    "seed:owners": "node backend/scripts/seedOwners.js",
    "seed:library": "node backend/scripts/seedLibrary.js",
    "seed:videos": "node backend/scripts/seedVideos.js",
    "seed:all": "npm run seed:owners && npm run seed:library && npm run seed:videos && npm run seed:events",
    "seed:production-owners": "node backend/seedProductionOwners.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "dashboard",
    "sign-company",
    "mern"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1477.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "ical-generator": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "redis": "^4.6.11"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1"
  }
}
